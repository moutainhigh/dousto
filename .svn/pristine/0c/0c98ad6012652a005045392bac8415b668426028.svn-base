package com.ibm.oms.client.dto.saleAfterOrder;

/**
 * 门店退换货单信息查询条件dto
 */
public class ShopSaleAfterOrderQueryDto implements java.io.Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -8195608902987654097L;
	
	//门店代码
	private String shopNo;
	
	//开始时间
	private String startDate;
	
	//结束时间
	private String endDate;
	
	//状态
	private String status;
	
	//渠道来源
	private String channelSource;
	
	//退货单号
	private String returnNo;
	
	//会员代号
	private String memberNo;
	
	//退货子单号
	private String returnItemNo;
	
	//退换货标识
	private String retChgFlg;

	public String getShopNo() {
		return shopNo;
	}

	public void setShopNo(String shopNo) {
		this.shopNo = shopNo;
	}

	public String getStartDate() {
		return startDate;
	}

	public void setStartDate(String startDate) {
		this.startDate = startDate;
	}

	public String getEndDate() {
		return endDate;
	}

	public void setEndDate(String endDate) {
		this.endDate = endDate;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getChannelSource() {
		return channelSource;
	}

	public void setChannelSource(String channelSource) {
		this.channelSource = channelSource;
	}

	public String getReturnNo() {
		return returnNo;
	}

	public void setReturnNo(String returnNo) {
		this.returnNo = returnNo;
	}

	public String getMemberNo() {
		return memberNo;
	}

	public void setMemberNo(String memberNo) {
		this.memberNo = memberNo;
	}

	public String getReturnItemNo() {
		return returnItemNo;
	}

	public void setReturnItemNo(String returnItemNo) {
		this.returnItemNo = returnItemNo;
	}

	public String getRetChgFlg() {
		return retChgFlg;
	}

	public void setRetChgFlg(String retChgFlg) {
		this.retChgFlg = retChgFlg;
	}
}