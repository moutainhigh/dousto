<mapping>
	<class name="com.ibm.oms.intf.intf.TmsPayDTO">
	<map-to xml="request" auto-complete="true" />
		<field name="logisticCompanyId" type="string">
		<bind-xml name="logisticCompanyId" node="element" /> 
		</field>
		<field name="money" type="double">
		<bind-xml name="money" node="element" /> 
		</field>
		<field name="occurtime" type="string">
		<bind-xml name="occurtime" node="element" /> 
		</field>
		<field name="operator" type="string">
		<bind-xml name="operator" node="element" /> 
		</field>
		<field name="paymode" type="integer">
		<bind-xml name="paymode" node="element" /> 
		</field>
		<field name="remark" type="string">
		<bind-xml name="remark" node="element" /> 
		</field>
		<field name="txLogisticID" type="string">
		<bind-xml name="txLogisticID" node="element" /> 
		</field>
	</class>
</mapping>