<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/decorators">
	<excludes>
		<pattern>/json/*</pattern>
		<pattern>/WEB-INF/decorators/*</pattern>
		<pattern>/sys/user!menu.action*</pattern>
		<pattern>/sys/user!header.action*</pattern>
		<pattern>/sys/user!main.action*</pattern>
		<pattern>/**login.action*</pattern>
	</excludes>
	<decorator name="index" page="index.ftl">
		<pattern>/*</pattern>
	</decorator>
</decorators>
